<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_enabled="false">
		<shape>
			<corners android:radius="100dp" />
			<gradient android:angle="315" android:endColor="@color/splunk_flamingo" android:startColor="@color/splunk_lipstick" />
			<padding android:bottom="8dp" android:left="20dp" android:right="20dp" android:top="8dp" />
		</shape>
	</item>
	<item>
		<ripple android:color="@color/white">
			<item android:id="@android:id/mask">
				<shape>
					<corners android:radius="100dp" />
					<solid android:color="#fff" />
				</shape>
			</item>
			<item>
				<shape>
					<corners android:radius="100dp" />
					<gradient android:angle="315" android:endColor="@color/splunk_flamingo" android:startColor="@color/splunk_lipstick" />
					<padding android:bottom="8dp" android:left="20dp" android:right="20dp" android:top="8dp" />
				</shape>
			</item>
		</ripple>
	</item>
</selector>