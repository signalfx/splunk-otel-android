{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": ["develop"],
  "schedule": ["monthly"],
  "packageRules": [
    // Exclude updates for gradle.
    {
      "matchPackageNames": ["com.android.tools.build:gradle", "com.android.tools.build:gradle-api"],
      "enabled": false
    }
  ]
}
